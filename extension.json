{
  "name": "Emulate Group",
  "version": "0.0.1",
  "author": [
    "Open Tech Strategies, LLC"
  ],
  "license-name": "GNU Affero General Public License",
  "url": "https://github.com/OpenTechStrategies/EmulateGroup",
  "descriptionmsg": "emulategroup-desc",
  "type": "other",
  "requires": {
    "MediaWiki": ">= 1.35.0"
  },
  "GroupPermissions": {
    "*": {
      "emulategroup": false
    }
  },
  "AvailableRights": [
    "emulategroup"
  ],
  "APIModules": {
    "emulategroupstart": "EmulateGroupStart",
    "emulategroupstop": "EmulateGroupStop"
  },
  "MessagesDirs": {
    "EmulateGroup": [
      "i18n"
    ]
  },
  "AutoloadClasses": {
    "EmulateGroup": "includes/EmulateGroup.php",
    "EmulateGroupStart": "includes/api/EmulateGroupStart.php",
    "EmulateGroupStop": "includes/api/EmulateGroupStop.php"
  },
  "ResourceModules": {
    "ext.emulategroup.js": {
      "scripts": "resources/js/EmulateGroup.js",
      "dependencies": [ "mediawiki.api" ],
      "messages": [],
      "targets": [
        "desktop",
        "mobile"
      ]
    }
  },
  "ResourceFileModulePaths": {
    "localBasePath": "",
    "remoteExtPath": "EmulateGroup"
  },
  "Hooks": {
    "LoadExtensionSchemaUpdates": "EmulateGroup::onLoadExtensionSchemaUpdates",
    "SidebarBeforeOutput": "EmulateGroup::onSidebarBeforeOutput",
    "BeforePageDisplay": "EmulateGroup::onBeforePageDisplay"
  },
  "config": {
    "EmulateGroupGroupList": false
  },
  "manifest_version": 1
}
